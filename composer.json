{
    "name": "saschaegerer/phpstan-typo3",
    "description": "TYPO3 CMS class reflection extension for PHPStan",
    "license": ["MIT"],
    "type": "phpstan-extension",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.1",
        "phpstan/phpstan": "^0.12",
        "typo3/cms-core": "^8.7 || ^9.5 || ^10.4 || 11.*.*@dev",
        "typo3/cms-extbase": "^8.7 || ^9.5 || ^10.4 || 11.*.*@dev"
    },
    "require-dev": {
        "consistence/coding-standard": "^3.8",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "phing/phing": "^2.16.0",
        "phpstan/phpstan-strict-rules": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "SaschaEgerer\\PhpstanTypo3\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.12-dev"
        },
        "phpstan": {
            "includes": ["extension.neon"]
        }
    }
}
